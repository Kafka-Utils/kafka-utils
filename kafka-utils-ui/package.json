{
  "name": "kafka-utils-ui",
  "version": "0.0.0",
  "config": {
    "projectName": "kafka-utils"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --prod --deploy-url \"/$npm_package_config_projectName/\" --base-href \"/$npm_package_config_projectName/\"",
    "test": "ng test --browsers=Chrome",
    "testNoWatch": "ng test --watch=false --browsers=ChromeHeadless",
    "e2e": "ng e2e",
    "dev": "NODE_OPTIONS='--max_old_space_size=2048' ng serve --proxy-config proxy.conf.js --port 4200  --deploy-url \"/$npm_package_config_projectName/\" --base-href \"/$npm_package_config_projectName/\"",
    "generateClient": "openapi-generator-cli generate -i ../build/tmp/kapt3/classes/main/META-INF/swagger/kafka-utils-0.0.yml -g typescript-angular -o generated-sources/kafka-utils-client --skip-validate-spec --additional-properties=ngVersion=12.0.1,npmName=kafkaUtilsRestClient,supportsES6=true,npmVersion=6.14.11,withInterfaces=true",
    "buildKafkaUtilsRestClient": "cd generated-sources/kafka-utils-client && npm run build && cd ../..",
    "preinstall": "npm run generateClient && npm run buildKafkaUtilsRestClient && npm install ./generated-sources/kafka-utils-client/dist"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.0.1",
    "@angular/cdk": "^12.0.1",
    "@angular/common": "~12.0.1",
    "@angular/compiler": "~12.0.1",
    "@angular/core": "~12.0.1",
    "@angular/forms": "~12.0.1",
    "@angular/material": "^12.0.1",
    "@angular/platform-browser": "~12.0.1",
    "@angular/platform-browser-dynamic": "~12.0.1",
    "@angular/router": "~12.0.1",
    "@openapitools/openapi-generator-cli": "^2.3.2",
    "kafkaUtilsRestClient": "file:generated-sources/kafka-utils-client/dist",
    "rxjs": "~6.6.0",
    "tslib": "^2.1.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.0.1",
    "@angular/cli": "~12.0.1",
    "@angular/compiler-cli": "~12.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/node": "^12.11.1",
    "jasmine-core": "~3.7.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "typescript": "~4.2.3"
  }
}
